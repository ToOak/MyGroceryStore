<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ViewfinderView2">
        <!--中部扫描框的左右边距-->
        <attr name="cameraPaddingX" format="dimension" />
        <!--中部扫描框的上下边距-->
        <attr name="cameraPaddingY" format="dimension" />
        <!--中部扫描框的高度-->
        <attr name="cameraHeight" format="dimension"/>
    </declare-styleable>

    <!-- 商品损坏原因 -->
    <string-array name="consume_reason">
        <item>外包装破损</item>
        <item>被当成赠品送掉</item>
        <item>商品被损坏</item>
        <item>商品意外遗失</item>
        <item>其它</item>
    </string-array>

</resources>